{
    "name": "youthweb/php-youthweb-api",
    "type": "library",
    "description": "Youthweb API client",
    "homepage": "https://github.com/youthweb/php-youthweb-api",
    "keywords": ["youthweb", "api"],
    "license": "GPL3",
    "authors": [
        {
            "name": "Artur Weigandt",
            "email": "art4@youthweb.net",
            "homepage": "http://wlabs.de"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "~6.0",
        "art4/json-api-client": "0.6.*",
        "cache/void-adapter": "^0.3.0",
        "youthweb/oauth2-youthweb": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4"
    },
    "autoload": {
        "psr-4": {
            "Youthweb\\Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Youthweb\\Api\\Tests\\Integration\\": "tests/integration",
            "Youthweb\\Api\\Tests\\Unit\\": "tests/unit"
        }
    }
}
